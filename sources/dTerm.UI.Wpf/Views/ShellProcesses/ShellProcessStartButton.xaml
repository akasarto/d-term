<local:ShellProcessStartButtonBase
    x:Class="dTerm.UI.Wpf.UserControls.ShellProcessStartButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:dTerm.UI.Wpf.UserControls"
    mc:Ignorable="d" 
    d:DesignHeight="450"
    d:DesignWidth="800"
>

    <Button Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" x:Name="launchButton">
        <materialDesign:PackIcon x:Name="launchButtonIcon" Height="24" Width="24" FocusManager.IsFocusScope="False" />
    </Button>

</local:ShellProcessStartButtonBase>
